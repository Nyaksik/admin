{
  "extends": "fastify-tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "paths": {
      "@/utils": ["src/utils/"],
      "@/schemas": ["src/schemas/"],
      "@/services": ["src/services/"],
      "@/types": ["src/types/"],
      "@/controllers": ["src/controllers/"],
    }
  },
  "include": ["src/**/*.ts"],
}
